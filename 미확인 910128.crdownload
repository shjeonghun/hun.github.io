				<script type="text/javascript">
					$(function(){

						var $ul = $(".slideCont02 .thum ul");
						var listWidth = $(".slideCont02 .thum li").size() * ( $(".slideCont02 .thum li").width()+2) -2;
						
						$ul.css("width",listWidth);
					
						var idx = 0
						var $list = $(".slideCont02 .thum li");
						var $lastIdx = $(".slideCont02 .thum li:last").index();
						var viewNum = 6;
						var hideNum = $lastIdx - viewNum;

						$(".slideCont02 .next").click(function(e){
							e.preventDefault();

							if(idx != $lastIdx){
								idx++;
								
								if(idx > viewNum ){
									$ul.stop().animate({
										marginLeft : (idx - viewNum) * -92 + "px"
									});
								}
							} else {
								idx = 0;
								
								$ul.stop().animate({
									marginLeft : "0px"
								});
							}
							move();
						});
						

						$(".slideCont02 .prev").click(function(e){
							e.preventDefault();
							
							if(idx == 0){
								idx = $lastIdx
								
								if ( $(".slideCont02 .thum li").size() > viewNum) {
									$ul.stop().animate({
										marginLeft : hideNum * -92 + "px"
									});
								}
								
							} else {
								idx--;

								if(idx < hideNum ){
									$ul.stop().animate({
										marginLeft : idx * -92 + "px"
									});
								}
							}
							
							move();
						});

						$(".slideCont02 .thum li div").click(function(e){
							e.preventDefault();
							idx = $(this).parent().index();

							move();
						});

						function move(){
							var $imgIdx = $list.eq(idx).children("img").attr("src");
							var $imgAlt = $list.eq(idx).children("img").attr("alt");							
							
							$(".slideCont02 .visual .img img").attr("src",$imgIdx);
							$(".slideCont02 .visual .text .tit").html($imgAlt);
	
							$list.eq(idx).addClass("on").siblings().removeClass("on");
							$(".slideCont02 .visual .firstNum").text(idx+1);
						}
					});
				</script>
			
				
